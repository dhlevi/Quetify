{
  "name": "quetify-client",
  "version": "1.0.0",
  "description": "A client for Quetify queue usage",
  "author": {
    "name": "dhlevi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhlevi/quetify.git"
  },
  "keywords": [
    "mongodb",
    "queues"
  ],
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run lint && rimraf dist/* && npm run build-ts && npm run test",
    "debug": "npm run build && npm run watch-debug",
    "test": "jest --forceExit --coverage --verbose",
    "lint": "tsc --noEmit && eslint \"**/*.ts\" --quiet --fix"
  },
  "dependencies": {
    "mongodb": "^3.6.5"
  },
  "devDependencies": {
    "@types/mongodb": "^3.6.12",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "rimraf": "3.0.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "~4.1.3"
  },
  "license": "Apache-2.0"
}
